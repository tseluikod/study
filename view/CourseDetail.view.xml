<mvc:View
  height="100%"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:f="sap.ui.layout.form"
  xmlns:core="sap.ui.core"
  >
  <Page
    title="Главная страница"
    class="marginBoxContent"
    showNavButton="true" >
    <headerContent>
    </headerContent>
    <subHeader>
    </subHeader>
    <content>
      <f:Form id="FormDisplay354"
        minWidth="1024"
        maxContainerCols="2"
        editable="false"
        class="isReadonly">
        <f:title>
          <core:Title text="CourseInfo" />
        </f:title>
        <f:layout>
          <f:ResponsiveGridLayout
            labelSpanL="3"
            labelSpanM="3"
            emptySpanL="4"
            emptySpanM="4"
            columnsL="1"
            columnsM="1" />
        </f:layout>
        <f:formContainers>
          <f:FormContainer>
            <f:formElements>
              <f:FormElement label="Name">
                <f:fields>
                  <Text text="{name}" id="nameText"  />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="Duration">
                <f:fields>
                  <Text text="{duration}" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="Level">
                <f:fields>
                  <Text text="{level}" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="Exercise">
                <f:fields>
                  <Text text="{exercise}" id="exerciseText" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="Teacher">
                <f:fields>
                  <Text text="{teacher}" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="Product">
                <f:fields>
                  <Text text="{product}" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="StartDate">
                <f:fields>
                  <Text text="{startDate}" />
                </f:fields>
              </f:FormElement>
              <f:FormElement label="EndDate">
                <f:fields>
                  <Text text="{endDate}"  />
                </f:fields>
              </f:FormElement>
             <!--  <f:FormElement label="Practice">
                <f:fields>
                  <Text text="{practice}"  />
                </f:fields>
              </f:FormElement> -->
            </f:formElements>
          </f:FormContainer>
        </f:formContainers>
      </f:Form>
    </content>
    <footer>
    </footer>
  </Page>
</mvc:View>